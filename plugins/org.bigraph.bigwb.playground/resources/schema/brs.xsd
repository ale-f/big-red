<xsd:schema xmlns:bigraph="http://www.itu.dk/research/pls/xmlns/2010/bigraph" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.itu.dk/research/pls/xmlns/2010/bigraph">

<xsd:group name="anyForeignElements">
		<xsd:sequence>
			<xsd:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
		</xsd:sequence>
	</xsd:group>            

<xsd:element name="bigraph">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:group ref="bigraph:anyForeignElements"/>
			</xsd:sequence>
			<xsd:attribute name="src" type="xsd:string"/>
			<xsd:anyAttribute namespace="##other" processContents="lax"/>
		</xsd:complexType>
	</xsd:element>

<xsd:element name="signature">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:group ref="bigraph:anyForeignElements"/>
			</xsd:sequence>
			<xsd:attribute name="src" type="xsd:string"/>
			<xsd:anyAttribute namespace="##other" processContents="lax"/>
		</xsd:complexType>
	</xsd:element>



</xsd:schema>
